<%= form_for(@jury_form) do |f| %>

  <div class="field">
    <%= f.label :applied_study_form_id %><br />
    <%= f.number_field :applied_study_form_id %>
  </div>
  <div class="field">
    <%= f.label :judge_id %><br />
    <%= f.number_field :judge_id %>
  </div>
  <div class="field">
    <%= f.label :criteria_id %><br />
    <%= f.number_field :criteria_id %>
  </div>
  <div class="field">
    <%= f.label :comments %><br />
    <%= f.text_field :comments %>
  </div>
  <div class="field">
    <%= f.label :final_assessment %><br />
    <% options = options_for_select([
      ['Unsatisfactory Progress at this level', 'Unsatisfactory Progress at this level'], 
      ['Continue study at this level', 'Continue study at this level',
      ['Advance to study at next level', 'Advance to study at next level'],
      ['All jury levels for degree completed', 'All jury levels for degree completed']], 4) %>
    <%= f.select :final_assessment, options %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
