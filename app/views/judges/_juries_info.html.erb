<% dummyInt = 0 %>

<div class="accordion" id="accordion2">
  <div class="accordion-group">
  	<% @judge.juries.each do |jury| %>
    <div class="accordion-heading">
      <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapse<%=dummyInt%>">
        <b> Jury for <%= Student.find(jury.student_id).name %> </b>
      </a>
    </div>
    <div id="collapse<%=dummyInt%>" class="accordion-body collapse">
      <div class="accordion-inner">
        <%= form_information(jury.jury_forms, @judge) %>
      </div>
    </div>
    <% dummyInt = dummyInt + 1 %>
    <% end %>
  </div>
</div>

