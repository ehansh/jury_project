<h1>Listing applied_study_forms</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Repertoire</th>
    <th>Studies and scales</th>
    <th>Public performances</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @applied_study_forms.each do |applied_study_form| %>
  <tr>
    <td><%= applied_study_form.student_id %></td>
    <td><%= applied_study_form.repertoire_id %></td>
    <td><%= applied_study_form.studies_and_scales %></td>
    <td><%= applied_study_form.public_performances %></td>
    <td><%= link_to 'Show', applied_study_form %></td>
    <td><%= link_to 'Edit', edit_applied_study_form_path(applied_study_form) %></td>
